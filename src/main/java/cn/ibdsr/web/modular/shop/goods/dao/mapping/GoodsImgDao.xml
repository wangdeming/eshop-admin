<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ibdsr.web.modular.shop.goods.dao.GoodsImgDao">
    <!--删除商品图片-->
    <update id="delete">
        UPDATE goods_img
        SET is_deleted = 1
        WHERE id = #{id}
    </update>

</mapper>